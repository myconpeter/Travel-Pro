<body>

    <section id="invoice">
        <div class="container my-10 py-10">

            <div class="text-center pb-5">
                <img src="images/logo_dark.png" alt="">
            </div>

			<h1	style=" margin-left: 10px ; text-align: center;"
						class="m-0 text-primary">
						<i
							class="fa fa-map-marker-alt me-3"
							style="margin-right: 10px"></i
						>Travel-Pro Logistics
					</h1>

            <div class="d-md-flex justify-content-between my-5">
                <div>
                    <p class="fw-bold text-primary">Invoice To</p>
                    <h4><%= item.name %></h4>
                    <ul class="list-unstyled m-0">
                        <li><%= item.to %></li>
                        <li><%= item.departure %></li>
                        <li><%= item.orderNumber %></li>
                    </ul>
                </div>
                <div class="mt-5 mt-md-0">
                    <p class="fw-bold text-primary">Invoice From</p>
                    <h4>Travel-Pro</h4>
                    <ul class="list-unstyled m-0">
                        <li>Shipment Logistics</li>
                        <li>Travel-Pro@gmail.com</li>
                    </ul>
                </div>
            </div>

            <div
                class=" d-md-flex justify-content-between align-items-center border-top border-bottom border-primary my-5 py-3">
                <h2 class="display-6 fw-bold m-0">Invoice <%= item.shipmentNo %> </h2>
                <div>
                    <p class="m-0"> <span class="fw-medium">Invoice No:</span> <%= item.shipmentNo %></p>
                    <p class="m-0"> <span class="fw-medium">Invoice Date:</span> <%= item.departure %></p>
                    <p class="m-0"> <span class="fw-medium">Shipment type:</span> <%= item.shipmentType %></p>
                    <p class="m-0"> <span class="fw-medium">Delivery Date:</span> <%= item.dueDate %></p>
                </div>

            </div>

            <div class="py-1">
                <table class="table table-striped border my-5">
                    <thead>
                        <tr>
                           
                            <th scope="col">Items Description</th>
                            <!-- <th scope="col">Price</th>
                            <th scope="col">Quantity</th>
                            <th scope="col">Total</th> -->
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                           
