                            <!-- <td>$350.00</td>
                            <td>2</td>
                            <td>$700.00</td> -->
                        </tr>
                        <tr>
                           
                            <td style="height: 40px;"><%= item.item2 %></td>
                            <!-- <td>$250.00</td>
